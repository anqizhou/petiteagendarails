            </div>
            <input id="task_item_task_starred" name="task_item[task_starred]" type="checkbox">
            <input id="task_item_user_id" name="task_item[user_id]" type="hidden" value="<%= current_user.id if current_user %>">
            <input class="addbutton_task" name="commit" type="submit" value="Add">
