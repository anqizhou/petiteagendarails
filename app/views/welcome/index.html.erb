<div class="container">
    <div class="board_section">
      <div class="tab_area">
        <ul class="nav nav-tabs" role="tablist">
          <li class="active"><a href="#">My activities</a></li>
          <li><a href="#">Exploreï¼ˆcoming soon)</a></li>
          <li><a href="#"><span class="glyphicon glyphicon-star glyphicon-star-filled"></span> Starred</a></li>
        </ul>
      </div>
      <div class="board_area">
        <!-- 1st board starts here: -->
        <div class="board activity_board board_1">

          <form action="/activity_items" method="post">
            <div class="board_title">
              <input name="activity_item[activity_item]" type="text" class="board_title_input" placeholder="Add activity..."><span class="glyphicon glyphicon-edit"></span>
            </div>
            <input id="activity_item_activity_color" name="activity_item[activity_color]" type="text">
            <div class="datetime_picker">
              <input name="activity_item[activity_date]" type="text" class="board_date_input" placeholder="Oct/25">
              <input name="activity_item[starting_time]" type="text" class="board_time_start_input" placeholder="10:00"> to
              <input name="activity_item[ending_time]" type="text" class="board_time_end_input" placeholder="11:30">
            </div>
            <div class="board_note">
              <textarea name="activity_item[activity_note]" class="board_note_input" rows="3" placeholder="Note"></textarea>
            </div>
            <input id="activity_item_activity_starred" name="activity_item[activity_starred]" type="checkbox">
            <input id="activity_item_user_id" name="activity_item[user_id]" type="hidden" value="<%= current_user.id if current_user %>">
            <input class="addbutton_activity" name="commit" type="submit" value="Add">
            <input name="authenticity_token" type="hidden" value="<%= form_authenticity_token %>">
          </form>

        </div>
        <!-- 1st board ends above -->

        <!-- 2nd board starts here: -->
        <div class="board board_2 task_board">
          <form action="/task_items" method="post">
